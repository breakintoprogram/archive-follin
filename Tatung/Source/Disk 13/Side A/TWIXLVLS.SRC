TWIX_A:         DB GATE_CON,255,ENDIT,255
                DB ENVON,1,ADSR,090H,001H,1
                DB SEND,7,00001000B
                DB FOR,2,GOSUB
                DW MARSE
                DB NEXT,TRANS,1
                DB FOR,2,GOSUB
                DW MARSE
                DB NEXT,TRANS,0,GOTO
                DW TWIX_A
                DB STOP
;.....................................
                
TWIX_B:         DB GATE_CON,255,ENDIT,255
                DB ENVON,1,ADSR,070H,0,2
                DB SEND,7,00010000B
                DB VIB,12,3,2,0
                DB FOR,2,GOSUB
                DW BIT
                DB NEXT,TRANS,1
                DB FOR,2,GOSUB
                DW BIT
                DB NEXT,TRANS,0,GOTO
