                DB STOP
;.....................................
                
TWIX_C:         DB GATE_CON,255,ENDIT,255
                DB ENVON,1,ADSR,090H,002H,1
                DB SEND,7,00100000B
                DB FOR,2,GOSUB
                DW FUNC
                DB NEXT,TRANS,1
                DB FOR,2,GOSUB
                DW FUNC
                DB NEXT,TRANS,0,GOTO
                DW TWIX_C
                
                DB STOP
;.....................................
                
BIT:            DB ADSR,0A0H,007H,1,55,24
                DB ADSR,080H,0,2
                DB 51,6,53,6
                DB 48,6,44,6,51,6,53,6
                DB 48,6,44,6
